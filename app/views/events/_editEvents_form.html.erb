<%= form_for(@events) do |f| %>
  <%= f.text_field :date, placeholder: 'Your Name here' %>
  <%= f.text_field :venue_name, placeholder: 'Your Name here' %>
  <%= f.text_field :address, placeholder: 'Your Name here' %>
  <%= f.text_field :compacity, placeholder: 'Your Name here' %>
  <%= f.text_field :attendance, placeholder: 'Your Name here' %>
  <%= f.text_field :settle, placeholder: 'Your Name here' %>
  <%= f.text_field :total, placeholder: 'Your Name here' %>
  <%= f.text_field :complete, placeholder: 'Your Name here' %>
  <%= f.text_field :tours_id, placeholder: 'Your Name here' %>
  <%= f.submit 'Edit your trip' %>
<%end %>
